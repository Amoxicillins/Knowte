<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Style x:Key="FontAwesome">
        <Setter Property="TextElement.FontFamily" Value="/Knowte.Common;component/Resources/Fonts/#FontAwesome" />
    </Style>
    
    <Style x:Key="SegoeAssets">
        <Setter Property="TextElement.FontFamily" Value="/Knowte.Common;component/Resources/Fonts/#Segoe MDL2 Assets" />
    </Style>

    <Style x:Key="RobotoThin">
        <Setter Property="TextElement.FontFamily" Value="/Knowte.Common;component/Resources/Fonts/#Roboto Thin" />
    </Style>
</ResourceDictionary>